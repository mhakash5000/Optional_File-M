import React, { Component } from 'react';

class ClassCom extends Component {
    
    fruitList=(data)=>{
        return <option>{data+5}</option>
    }
    render() {
     
        const number=['1','2','3','4','7','8','9','10'];
        const allNumber=number.map(this.fruitList);
       

        return (
            <div>
                 <select>
                     {allNumber}
                 </select>
            </div>
        );
    }
}

export default ClassCom;
