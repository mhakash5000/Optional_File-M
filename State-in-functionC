import React from 'react';
import { useState } from 'react';

const Rhook = () => {


    //destructing assignment
    const[value,setValue]=useState({
        name:"Bangladesh"
    })

const Change=()=>{
   setValue({
       name:"NewsLand",
   })
}
    return (
        <div>
            <h1>{value.name}</h1>
            <button onClick={Change}>ChangeValue</button>
        </div>
    );
};

export default Rhook;
