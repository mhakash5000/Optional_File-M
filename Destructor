import React from 'react';

const Course = (props) => {
    const {name, instructor, image, price}=props.course;
    return (
        <div>  
            <h1>Name: {name}</h1>
            <h1>Instructor: {instructor}</h1>
            <h1>Image: {image}</h1>
            <h1>Price: {price}</h1>
        </div>
    );
};

export default Course;
