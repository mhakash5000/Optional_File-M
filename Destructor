import React from 'react';

const Course = (props) => {
    const {name, instructor, image, price } = props.course;
    return (
        <div>  
            <div>
                <img src={image} alt=""/>
            </div>

            <div>
                <h1>Name: {name}</h1>
                <h1>Instructor: {instructor}</h1>
                <h1>Price: {price}</h1>
            </div>
        </div>
    );
};

export default Course;
